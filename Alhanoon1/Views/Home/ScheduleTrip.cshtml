@model CommonUtility.LandingPageModel
@{
    ViewBag.Title = "ScheduleTrip";
    Layout = "~/Views/Shared/_Masterlayout.cshtml";
}

<style>
		.breadcrumb {
			margin: 0;
		}
		.main .container {
			border-bottom: 1px solid #ccc;
			padding-bottom: 20px;
		}
		.inline ul li {
			display: inline-block;
			padding: 0px 3px;
			color: #60646D;
		}
		.inline ul li i {
			color: #65C9E3;
		}
		.inline ul {
			padding: 6px 0px;
		}
	
</style>
<div class="clearfix"></div>

<div class="main">
    <div class="container">
        <div class="breadcrumb">

            <ul>
                <li>

                    <a href="@Url.Action("Index","Home")">Home</a>
                </li>
                <li>
                    /
                </li>
                <li>
                    Treatment
                </li>
            </ul>
        </div>
        <div class="clearfix"></div>
        <div class="offer schedule">
            @foreach(var item in Model.ScheduleTripList)
            { 
            <div class="heading2">
                <h4>@item.Title</h4>
            </div>

                  @Html.Raw(item.Description)
          

            }
        </div>

    </div>
</div>